<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.uka.ipd.sdq.featuremodel.presentation.featuremodelModelWizardID"
            name="%_UI_featuremodelModelWizard_label"
            class="de.uka.ipd.sdq.featuremodel.presentation.featuremodelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/featuremodelModelFile.gif">
         <description>%_UI_featuremodelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="de.uka.ipd.sdq.featuremodel.presentation.featuremodelEditorID"
            name="%_UI_featuremodelEditor_label"
            icon="icons/full/obj16/featuremodelModelFile.gif"
            extensions="featuremodel"
            class="de.uka.ipd.sdq.featuremodel.presentation.featuremodelEditor"
            contributorClass="de.uka.ipd.sdq.featuremodel.presentation.featuremodelActionBarContributor">
      </editor>
   </extension>

</plugin>
